<Conditions>
  <Condition>
    <Name>负数发票</Name>
    <Field>合计金额</Field>
    <Operator>小于</Operator>
    <Value ValueType="Int">0</Value>
  </Condition>
  <Condition>
    <Name>作废发票</Name>
    <Field>作废标志</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">是</Value>
  </Condition>
  <Condition>
    <Name>按税率查询</Name>
    <Field>税率</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">|参数|</Value>
  </Condition>
  <Condition>
    <Name>正数发票</Name>
    <Field>合计金额</Field>
    <Operator>大于等于</Operator>
    <Value ValueType="Int">0</Value>
  </Condition>
  <Condition>
    <Name>正常发票</Name>
    <Field>作废标志</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">否</Value>
  </Condition>
  <Condition>
    <Name>按开票人查询</Name>
    <Field>开票人</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">|参数|</Value>
  </Condition>
  <Condition>
    <Name>带有销货清单</Name>
    <Field>清单标志</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">是</Value>
  </Condition>
  <Condition>
    <Name>尚未打印的发票</Name>
    <Field>打印标志</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">否</Value>
  </Condition>
  <Condition>
    <Name>从金税卡底层修复的发票</Name>
    <Field>修复标志</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">是</Value>
  </Condition>
  <Condition>
    <Name>导入的发票</Name>
    <Field>外开标志</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">是</Value>
  </Condition>
  <Condition>
    <Name>按开票日期查询</Name>
    <Field>开票日期</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">|参数|</Value>
  </Condition>
  <Condition>
    <Name>按发票种类查询</Name>
    <Field>发票种类</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">q</Value>
  </Condition>
  <Condition>
    <Name>专用发票</Name>
    <Field>发票种类</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">s</Value>
  </Condition>
  <Condition>
    <Name>普通发票</Name>
    <Field>发票种类</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">c</Value>
  </Condition>
  <Condition>
    <Name>电子增值税普通发票</Name>
    <Field>发票种类</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">p</Value>
  </Condition>
  <Condition>
    <Name>增值税普通发票(卷票)</Name>
    <Field>发票种类</Field>
    <Operator>等于</Operator>
    <Value ValueType="String">q</Value>
  </Condition>
  <Condition>
    <Name>负数废票</Name>
    <Field>负数发票</Field>
    <Operator>并且</Operator>
    <Value ValueType="DataField">作废发票</Value>
  </Condition>
  <Condition>
    <Name>正数废票</Name>
    <Field>正数发票</Field>
    <Operator>并且</Operator>
    <Value ValueType="DataField">作废发票</Value>
  </Condition>
  <Condition>
    <Name>正数正常票</Name>
    <Field>正数发票</Field>
    <Operator>并且</Operator>
    <Value ValueType="DataField">正常发票</Value>
  </Condition>
  <Condition>
    <Name>负数正常票</Name>
    <Field>负数发票</Field>
    <Operator>并且</Operator>
    <Value ValueType="DataField">正常发票</Value>
  </Condition>
</Conditions>