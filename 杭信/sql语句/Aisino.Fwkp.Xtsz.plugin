        <MenuItem id="Menu.Xtsz.HYXXBPrintEntry" type="Command" class="Aisino.Fwkp.Xtsz.Entry.HYXXBPrintEntry" label="红字货物运输业增值税专用发票信息表模板设置" isFirst="true" icon="系统设置_基本参数设置"/>
        </MenuItem>
    </MenuItem>
  </Menu>
	<Tree>
	  <TreeNode id="Menu.Xtsz" type="Menu" label="系统设置" isFirst="true" icon="系统设置_Tree">
