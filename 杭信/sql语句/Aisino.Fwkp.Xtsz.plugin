        <MenuItem id="Menu.Xtsz.BaseParasSet" type="Command" class="Aisino.Fwkp.Xtsz.ParaSetEntry" label="基本参数设置" isFirst="true" icon="系统设置_基本参数设置"/>
      </MenuItem>
      <MenuItem id="Menu.XtszSet.PrintSet" type="Menu" label="打印模板参数设置" icon="系统设置_系统参数设置">
