<?xml version="1.0" encoding="utf-8" ?>
<PlugIn id="Aisino.Fwkp.Xtsz" name="系统设置" description="系统设置插件">
  <Runtime>
    <Import assembly="..\Aisino.Fwkp.Xtsz.dll"/>
    <SqlLib file="Aisino.Fwkp.Xtsz.Sql.xml"/>
  </Runtime>
  <Auto>
    <Class id="Aisino.Fwkp.Xtsz.ShareMethodEntry" class="Aisino.Fwkp.Xtsz.ShareMethodEntry" lable="用户权限管理公共接口" alwaysPermit="true"></Class>
  </Auto>
	<Menu>
    <MenuItem id="Menu.Xtsz" type="Menu" label="系统设置" isFirst="true">
      <MenuItem id="Menu.Xtsz.SystemSet" type="Command" class="Aisino.Fwkp.Xtsz.SystemSetEntry" label="系统初始化" insertAfter="Menu.Xtgl.Xtzc" icon="系统设置_系统初始化"/>
