        <MenuItem id="Menu.Xtsz.JDCOldPrintSetEntry" type="Command" class="Aisino.Fwkp.Xtsz.Entry.JDCOldPrintSetEntry" label="旧版机动车销售统一发票模板设置" isFirst="true" icon="系统设置_基本参数设置"/>
        <MenuItem id="Menu.Xtsz.JDCNewPrintSetEntry" type="Command" class="Aisino.Fwkp.Xtsz.Entry.JDCNewPrintSetEntry" label="新版机动车销售统一发票模板设置" isFirst="true" icon="系统设置_基本参数设置"/>
        <MenuItem id="Menu.Xtsz.QDPrintSetEntry" type="Command" class="Aisino.Fwkp.Xtsz.Entry.QDPrintSetEntry" label="发票清单模板设置" isFirst="true" icon="系统设置_基本参数设置"/>
