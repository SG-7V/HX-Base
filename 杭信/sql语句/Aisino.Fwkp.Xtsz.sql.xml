  <!--增加记录到系统税务信息表-->
  <SQL id="aisino.Fwkp.Xtsz.AddXTSWXX">
    <value>
      INSERT INTO [XTSWXX](
      [QYBH], [QYMC], [ZCLX], [KPXE], [BSRQ], [BSRPHF], [YHZH], [FRDB], [YYDZ], [DHHM], [JYZS], [KJZG]
      )VALUES(
      @QYBH, @QYMC, @ZCLX, @KPXE, @BSRQ, @BSRPHF, @YHZH, @FRDB, @YYDZ, @DHHM, @JYZS, @KJZG
      )
    </value>
    <type>Text</type>
    <param name="QYBH" type="AnsiString"></param>
    <param name="QYMC" type="AnsiString"></param>
    <param name="ZCLX" type="AnsiString"></param>
    <param name="KPXE" type="Int32"></param>
    <param name="BSRQ" type="DateTime"></param>
    <param name="BSRPHF" type="Int32"></param>
    <param name="YHZH" type="AnsiString"></param>
    <param name="FRDB" type="AnsiString"></param>
    <param name="YYDZ" type="AnsiString"></param>
    <param name="DHHM" type="AnsiString"></param>
    <param name="JYZS" type="Boolean"></param>
    <param name="KJZG" type="AnsiString"></param>
  </SQL>

  <!--查询系统税务信息-->
  <SQL id="aisino.Fwkp.Xtsz.SelectXTSWXX">
    <value>
      SELECT * FROM [XTSWXX] WHERE [QYBH] = @QYBH
    </value>
    <type>Text</type>
    <param name="QYBH" type="AnsiString"></param>
  </SQL>

  <!--更新系统税务信息-->
  <SQL id="aisino.Fwkp.Xtsz.UpdateXTSWXX">
    <value>
      UPDATE [XTSWXX] SET [QYBH] = @QYBH, [YYDZ] = @YYDZ, [DHHM] = @DHHM, [YHZH] = @YHZH 
    </value>
    <type>Text</type>
    <param name="QYBH" type="AnsiString"></param>
    <param name="YYDZ" type="AnsiString"></param>
    <param name="DHHM" type="AnsiString"></param>
    <param name="YHZH" type="AnsiString"></param>
  </SQL>

  <!--获取电子底账参数信息-->
  <SQL id="aisino.Fwkp.Xtsz.SelectDZDZXX">
    <value>
      SELECT * FROM [XTDZDZXX] LIMIT 1
    </value>
    <type>Text</type>
  </SQL>

  <!--更新电子底账数据库-->
  <SQL id="aisino.Fwkp.Xtsz.UpdateXTDZDZXX">
    <value>
      UPDATE [XTDZDZXX] SET [ACCEPT_WEB_SERVER]=@ACCEPT_WEB_SERVER, [UPLOADNOW]=@UPLOADNOW, [INTERVALFLAG]=@INTERVALFLAG, [INTERVALTIME]=@INTERVALTIME, [ACCUMULATEFLAG]=@ACCUMULATEFLAG, [ACCUMULATENUM]=@ACCUMULATENUM, [DATASIZE]=@DATASIZE
    </value>
    <type>Text</type>
    <param name="ACCEPT_WEB_SERVER" type="AnsiString"></param>
    <param name="UPLOADNOW" type="Boolean"></param>
    <param name="INTERVALFLAG" type="Boolean"></param>
    <param name="INTERVALTIME" type="Int32"></param>
    <param name="ACCUMULATEFLAG" type="Boolean"></param>
    <param name="ACCUMULATENUM" type="Int32"></param>
    <param name="DATASIZE" type="Int32"></param>
  </SQL>

  <!--查询电子底账信息是否存在-->
